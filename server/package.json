{
  "name": "kinosu-server",
  "version": "1.0.0",
  "description": "Kinosu backend server",
  "main": "src/index.ts",
  "scripts": {
    "start": "node --no-deprecation dist/index.js",
    "dev": "nodemon --no-deprecation src/index.ts",
    "prebuild": "npm install @types/express @types/cors @types/bcryptjs @types/jsonwebtoken @types/nodemailer @types/uuid @types/multer --no-save",
    "build": "tsc",
    "serve": "npm run build && node --no-deprecation dist/index.js",
    "build:full": "cd .. && npm run build:server && cd server && npm run build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.19",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
